generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "mysql"
  url      = env("MAIN_DATABASE_URL")
}

model carts {
  id             Int      @id @default(autoincrement()) @db.UnsignedInt
  customerid     String?  @db.VarChar(255)
  quantity       Int?
  price          Decimal? @db.Decimal(10, 2)
  banner         String?  @db.VarChar(255)
  customer_name  String?  @db.VarChar(255)
  customer_email String?  @db.VarChar(255)
  customer_phone Int?
  productid      Int
  discount       Decimal  @db.Decimal(10, 2)
}

model categories {
  id          Int     @id @default(autoincrement()) @db.UnsignedInt
  title       String? @db.VarChar(255)
  description String? @db.LongText
  slug        String  @unique(map: "UQ_p_slug") @db.VarChar(255)
  banner      String  @db.VarChar(255)
}

model cloud_images {
  id   Int    @id @default(autoincrement())
  fid  Int
  type String @db.VarChar(50)
  url  String @db.VarChar(255)
}

model files {
  id                Int       @id @default(autoincrement()) @db.UnsignedInt
  name              String?   @db.VarChar(255)
  alternative_text  String?   @db.VarChar(255)
  caption           String?   @db.VarChar(255)
  width             Int?
  height            Int?
  formats           String?   @db.LongText
  hash              String?   @db.VarChar(255)
  ext               String?   @db.VarChar(255)
  mime              String?   @db.VarChar(255)
  size              Decimal?  @db.Decimal(10, 2)
  url               String?   @db.VarChar(255)
  preview_url       String?   @db.VarChar(255)
  provider          String?   @db.VarChar(255)
  provider_metadata String?   @db.LongText
  folder_path       String?   @db.VarChar(255)
  created_at        DateTime? @db.DateTime(6)
  updated_at        DateTime? @db.DateTime(6)
  created_by_id     Int?      @db.UnsignedInt
  updated_by_id     Int?      @db.UnsignedInt

  @@index([created_at], map: "upload_files_created_at_index")
  @@index([ext], map: "upload_files_ext_index")
  @@index([name], map: "upload_files_name_index")
  @@index([size], map: "upload_files_size_index")
  @@index([updated_at], map: "upload_files_updated_at_index")
}

model inventories {
  id        Int    @id @default(autoincrement()) @db.UnsignedInt
  stock     Int?
  productid Int    @unique(map: "productid")
  sku       String @db.VarChar(50)
}

model order_details {
  id         Int      @id @default(autoincrement()) @db.UnsignedInt
  quantity   Int?
  price      Decimal? @db.Decimal(10, 2)
  customerid String?  @db.VarChar(255)
  orderid    Int?
  productid  Int?
}

model orders {
  id               Int       @id @default(autoincrement()) @db.UnsignedInt
  customerid       String?   @db.VarChar(255)
  date_time        DateTime? @db.DateTime(6)
  status           String?   @db.VarChar(255)
  shipping_address String?   @db.LongText
  billing_address  String?   @db.LongText
  payment_method   String?   @db.VarChar(255)
  total_amount     Decimal?  @db.Decimal(10, 2)
  customer_name    String?   @db.VarChar(255)
  customer_email   String?   @db.VarChar(255)
  customer_phone   Int?
  tracking_no      String    @db.VarChar(50)
}

model product_tags {
  id        Int    @id @default(autoincrement())
  productid Int
  tagid     Int
  tagname   String @db.VarChar(50)
}

model products {
  id             Int      @id @default(autoincrement()) @db.UnsignedInt
  slug           String   @unique(map: "UQ_product_slug") @db.VarChar(50)
  banner         String   @db.VarChar(255)
  description    String?  @db.LongText
  ribbon         String   @db.VarChar(25)
  categoryid     Int
  name           String?  @db.VarChar(255)
  price          Decimal? @db.Decimal(10, 2)
  onsale         Boolean?
  discount       Decimal? @db.Decimal(10, 2)
  discounttype   String?  @db.VarChar(255)
  shippinginfo   String?  @db.LongText
  size           String   @db.VarChar(100)
  adddescription String   @db.VarChar(5000)
}

model reviews {
  id              Int       @id @default(autoincrement()) @db.UnsignedInt
  customerid      String?   @db.VarChar(255)
  customer_name   String?   @db.VarChar(255)
  rating          Int?
  review          String?   @db.LongText
  productid       Int?
  created_at      DateTime? @db.DateTime(6)
  updated_at      DateTime? @db.DateTime(6)
  published_at    DateTime? @db.DateTime(6)
  customer_avatar String?   @db.VarChar(255)
}

model shippings {
  id               Int       @id @default(autoincrement()) @db.UnsignedInt
  shipping_date    DateTime? @db.Date
  shipping_method  String?   @db.VarChar(255)
  shipping_address String?   @db.VarChar(255)
  tracking_number  Int?
  created_at       DateTime? @db.DateTime(6)
  updated_at       DateTime? @db.DateTime(6)
  orderid          Int?
}

model sliders {
  id          Int    @id @default(autoincrement())
  title       String @db.VarChar(50)
  description String @db.VarChar(255)
  banner      String @db.VarChar(255)
  link        String @db.VarChar(255)
  background  String @db.VarChar(50)
}

model tags {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(50)
}

model footer {
  id           Int    @id @default(autoincrement())
  address1     String @db.VarChar(255)
  address2     String @db.VarChar(255)
  supportemail String @db.VarChar(255)
  phone        String @db.VarChar(255)
  fb           String @db.VarChar(255)
  ig           String @db.VarChar(255)
  yt           String @db.VarChar(255)
  x            String @db.VarChar(255)
}
